{
  "name": "acmcsuf.com",
  "description": "Official website of CSUF's ACM club.",
  "version": "6.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://acmcsuf.com/",
  "author": "EthanThatOneKid (https://etok.codes/)",
  "repository": {
    "url": "git+https://github.com/EthanThatOneKid/acmcsuf.com.git"
  },
  "bugs": {
    "url": "https://github.com/EthanThatOneKid/acmcsuf.com/issues"
  },
  "main": "svelte.config.js",
  "keywords": [
    "svelte"
  ],
  "license": "MIT",
  "size-limit": [
    {
      "path": "node_modules",
      "limit": "256 MB"
    }
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:size": "npm i && size-limit",
    "test:coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore,unused-export-let:error\"",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "host": "npm run all && npm run preview",
    "all": "npm run lint && npm run format && npm run check && npm run build && npm t",
    "generate:colors": "node scripts/generate-colors/index.js"
  },
  "devDependencies": {
    "@size-limit/file": "^8.2.6",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/eslint": "^9.6.0",
    "@types/html-to-text": "^9.0.1",
    "@types/qrcode": "^1.5.1",
    "@types/rss": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "css": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.70.0",
    "size-limit": "^8.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@googleapis/calendar": "^9.8.0",
    "@js-temporal/polyfill": "^0.4.4",
    "cropperjs": "^2.0.0",
    "googleapis": "^146.0.0",
    "html-to-text": "^9.0.5",
    "lc-dailies": "github:acmcsufoss/lc-dailies#npm",
    "pomo": "github:ethanthatonekid/pomo#npm",
    "qrcode": "^1.5.4",
    "rrule": "^2.8.1",
    "rss": "^1.2.2",
    "svelte-confetti": "^2.3.1",
    "svelte-meta-tags": "^4.2.0"
  }
}
